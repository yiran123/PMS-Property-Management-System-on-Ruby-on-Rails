<p id="notice"><%= notice %></p>

<p>
  <strong>Room num:</strong>
  <%= @room.room_num %>
</p>


<%= link_to 'Edit', edit_room_path(@room) %> |
<%= link_to 'Back', rooms_path %>
<table>
<tbody>
    <% @room.assignments.each do |assignment| %>
      <tr>

      <th>Guest</th>
      <th colspan="3"></th>
        <td><%= assignment.guest.first_name + " " + assignment.guest.last_name%></td>
        
      </tr>
    <% end %>
  </tbody>

</table>
